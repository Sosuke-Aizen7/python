{
    "manifest_version": 2,
    "name": "Voice Assistant",
    "version": "1.0",
    "description": "A simple voice-controlled Chrome extension.",
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ]
  }
  